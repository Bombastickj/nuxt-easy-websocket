{
  "name": "nuxt-easy-websocket",
  "version": "2.3.0",
  "description": "Easy WebSocket integration module for nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bombastickj/nuxt-easy-websocket.git"
  },
  "license": "MIT",
  "author": "Jonas Brussat, Alexander FÃ¼hrer",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "build": {
    "externals": [
      "#nuxt-easy-websocket/routes"
    ]
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.3",
    "crossws": "latest",
    "defu": "^6.1.4",
    "knitwork": "^1.1.0",
    "mlly": "^1.7.3",
    "pathe": "^2.0.1",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.1",
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.3",
    "@nuxt/test-utils": "^3.14.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.15.0",
    "nuxt": "^3.15.3",
    "typescript": "latest",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0"
  }
}
