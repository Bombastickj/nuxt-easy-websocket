{
  "name": "nuxt-easy-websocket",
  "version": "4.0.0-alpha",
  "description": "Easy WebSocket integration module for nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bombastickj/nuxt-easy-websocket.git"
  },
  "license": "MIT",
  "author": "Jonas Brussat, Alexander FÃ¼hrer",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release:alpha": "npm run lint && npm run test && npm run prepack && changelogen --release --versionSuffix alpha && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "crossws": "^0.4.1",
    "defu": "^6.1.4",
    "knitwork": "^1.1.0",
    "pathe": "^2.0.1",
    "scule": "^1.3.0",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.0",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.14.4",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.15.0",
    "nuxt": "^4.0.3",
    "typescript": "latest",
    "vitest": "^3.0.4",
    "vue-tsc": "^3.0.1"
  },
  "overrides": {
    "crossws": "^0.4.1"
  }
}
